{
  "name": "notepad",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dx": "pnpm db-up && pnpm db-migrate-dev && pnpm db-seed && pnpm dev",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "deploy-gh-pages": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "lint": "next lint",
    "lint-fix": "pnpm lint --fix",
    "migrate": "prisma migrate deploy",
    "migrate-dev": "prisma migrate dev",
    "start": "next start",
    "studio": "prisma studio",
    "test": "playwright test",
    "test-ct": "playwright test -c playwright-ct.config",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "prettier": "prettier --write .",
    "postinstall": "pnpm generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.11.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "allotment": "^1.18.1",
    "codemirror": "^5.57.0",
    "csp-header": "^5.1.0",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.12.4",
    "jotai": "^2.0.4",
    "next": "^13.4.1",
    "next-auth": "^4.21.0",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-markdown": "^8.0.7",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "superjson": "1.12.3",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt-cli": "^17.5.0",
    "@playwright/test": "^1.32.3",
    "@types/codemirror": "^5.60.7",
    "@types/eslint": "^8.21.3",
    "@types/node": "^20.1.1",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.10.3"
  }
}
